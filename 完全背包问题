import java.util.Scanner;

public class Main {
    static int N=1010;
    static int [][]f=new int[N][N];
    static int []dp=new int[N];
    static int[] v=new int[N];
    static int[] w=new int[N];

    public static void main(String args[]) {
        Scanner in = new Scanner(System.in);
        int n = in.nextInt();
        int m = in.nextInt();

//        for (int i = 1; i <= n; i++) {
